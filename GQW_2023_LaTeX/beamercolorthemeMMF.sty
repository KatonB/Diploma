\mode<presentation>

% University of Udine theme colors
\definecolor{UniBrown}{RGB}{72,57,32}
\definecolor{UniOrange}{RGB}{239,125,45}
\definecolor{UniBlue}{RGB}{119,154,171}
\definecolor{UniGold}{RGB}{167,145,105}
%MMD colors
\definecolor{MMFGreen}{RGB}{84, 182, 134}
\definecolor{MMFblack}{RGB}{34, 34, 34}
\definecolor{MMFgray}{RGB}{151, 151, 151}
\definecolor{MMFApricot}{RGB}{255, 199, 179}
\definecolor{MMFMintApple}{RGB}{145, 207, 173}
% Normal text
\setbeamercolor{normal text}{fg=MMFblack}

% First page
\setbeamercolor*{title}{fg=MMFGreen,bg=gray}
\setbeamercolor*{author}{fg=MMFblack,bg=gray}
\setbeamercolor*{emailetc}{fg=MMFgray,bg=gray}
\setbeamercolor*{date}{fg=MMFblack,bg=gray}

% Generic frames header and footer
\setbeamercolor{titlelike}{fg=MMFblack}
\setbeamercolor*{frametitle}{fg=white}
\setbeamercolor*{framesubtitle}{fg=white}
\setbeamercolor*{number in head/foot}{fg=MMFblack}
\setbeamercolor*{title in head/foot}{fg=MMFgray}
\setbeamercolor*{author in head/foot}{fg=MMFgray}

%Blocks
\setbeamercolor{block title}{bg=MidnightBlue!30,fg=black}
\setbeamercolor{block title alerted}{bg=MMFApricot,fg=black}
\setbeamercolor{block title example}{bg=MMFMintApple,fg=black}
\setbeamercolor{block body}{bg=MidnightBlue!5,fg=black}
%\setbeamercolor{subsection in toc}{fg=MMFGreen}
\setbeamercolor{block body alerted}{bg=Red!5,fg=black}
\setbeamercolor{block body example}{bg=ForestGreen!5,fg=black}

% Lists
\setbeamercolor*{item}{fg=MMFGreen}

% Quotation and quote
\setbeamercolor*{quotation}{fg=black,bg=MidnightBlue!5}

% Verse
\setbeamercolor*{verse}{fg=black,bg=MidnightBlue!5}

% Abstract
\setbeamercolor*{abstract}{fg=black,bg=MidnightBlue!5}

% Table of contents 
\setbeamercolor*{structure}{fg=black}

% Bibliography
\setbeamercolor*{bibliography entry title}{fg=black}
\setbeamercolor*{bibliography entry author}{fg=black}
\setbeamercolor*{bibliography entry location}{fg=black!50}
\setbeamercolor*{bibliography entry note}{fg=black!50}


\mode
<all>